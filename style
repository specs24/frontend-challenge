/**changing box-sizing to border-box***/
*, *:before, *:after {
  box-sizing: border-box;
}

blockquote, q {
  quotes: none;
}

blockquote:before, blockquote:after,
q:before, q:after {
  content: '';
  content: none;
}


body {
    width: 320px;
    font: 12pt Helvetica, sans-serif;
} 

table {
  border-collapse: collapse;
  border-spacing: 0;
}